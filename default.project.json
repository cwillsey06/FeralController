{
  "name": "FeralController",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "common": {
        "$path": "src/common",
        "$ignoreUnknownInstances": true,

        "Packages": {
          "$path": "Packages"
        },

        "RequestModel": {
          "$className": "RemoteEvent"
        }
      }
    },

    "ServerScriptService": {
      "Server": {
        "$path": "src/Server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Client": {
          "$path": "src/Client"
        }
      }
    },
    
    "Players": {
      "$className": "Players",
      "$properties": {
        "CharacterAutoLoads": false
      }
    }
  }
}